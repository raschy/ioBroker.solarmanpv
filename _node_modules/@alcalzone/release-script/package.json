{
  "name": "@alcalzone/release-script",
  "version": "2.2.2",
  "description": "Release script to automatically increment version numbers and push git tags of Node.js projects",
  "main": "build/release.js",
  "bin": "bin/release.js",
  "files": [
    "/bin",
    "/build"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlCalzone/release-script.git"
  },
  "keywords": [
    "release",
    "automation",
    "semver"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlCalzone/release-script/issues"
  },
  "homepage": "https://github.com/AlCalzone/release-script#readme",
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.12.5",
    "@types/semver": "^7.3.6",
    "@types/yargs": "^17.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "alcalzone-shared": "^4.0.0",
    "axios": "^0.21.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "picocolors": "1.0.0",
    "semver": "^7.3.5",
    "yargs": "^17.0.1"
  },
  "scripts": {
    "build": "tsc",
    "watch": "npm run build -- --watch",
    "check": "npm run build -- --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npm run build && node bin/release.js"
  },
  "publishConfig": {
    "access": "public"
  }
}